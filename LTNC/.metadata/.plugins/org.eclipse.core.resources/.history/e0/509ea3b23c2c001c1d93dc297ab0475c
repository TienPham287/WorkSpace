package Btapthem;

public class _16 {
public static int getSn1(int n) {
	if (n==0)
		return 0;
	
	return getSn1(n-1)+n;
}
public static int getSn2(int n) {
	if (n==0)
		return 1;
	
	return getSn2(n-1)+n*n;
}
public static void main(String[] args) {
	System.out.println(getSn1(3));
	System.out.println(getSn2(2));
}
}
